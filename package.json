{
	"name": "user-manager",
	"version": "1.0.0",
	"description": "User Management System",
	"main": "index.js",
	"scripts": {
		"start": "node index.js",
		"server": "nodemon index.js",
		"client": "npm run start --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"build": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
		"heroku-postbuild": "npm run build"
	},
	"keywords": [
		"user",
		"management",
		"system"
	],
	"author": "Adams Banjo",
	"license": "ISC",
	"dependencies": {
		"body-parser": "^1.20.1",
		"cloudinary": "^1.13.2",
		"concurrently": "^4.1.0",
		"cookie-session": "^2.0.0-beta.3",
		"cors": "^2.8.5",
		"datauri": "^1.1.0",
		"express": "^4.18.2",
		"express-fileupload": "^1.1.1-alpha.1",
		"lodash": "^4.17.11",
		"mongoose": "^5.4.5",
		"multer": "^1.4.1",
		"passport": "^0.4.0",
		"passport-google-oauth20": "^1.0.0",
		"path-parser": "^4.2.0",
		"redis": "^2.8.0",
		"uuid": "^3.3.2"
	},
	"devDependencies": {
		"nodemon": "^1.18.9"
	}
}
